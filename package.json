  "license": "Apache-2.0",
  "devDependencies": {
    "dependency-check": "^3.1.0",
    "nodemon": "^1.17.5",
    "npm-check-updates": "^2.14.2",
    "standard": "^11.0.1"
  },
  "dependencies": {
    "body": "^5.1.0",
    "cookie": "^0.3.1",
    "dockerode": "^2.5.5",
    "jsonwebtoken": "^8.3.0",
    "kubernetes-client": "^5.3.0",
    "merry": "^5.5.2",
    "path-is-inside": "^1.0.2",
    "pino": "^4.17.3",
    "random-port": "^0.1.0",
    "retry-request": "^4.0.0",
    "send": "^0.16.2"
  }
