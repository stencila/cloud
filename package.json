{
  "name": "stencila-cloud",
  "version": "0.27.0",
  "description": "Stencila in the cloud",
  "scripts": {
    "lint": "tslint --project tsconfig.json --fix --format stylish ./src/**/*.ts",
    "test": "jest",
    "cover": "jest --collectCoverage",
    "build": "tsc",
    "docs": "typedoc --readme README.md --out ./docs ./src",
    "start": "if [ \"$NODE_ENV\" = \"development\" ]; then ts-node-dev src/server.ts; else node dist/server.js; fi",
    "deps-uptodate": "ncu",
    "deps-used": "npm run build && dependency-check --entry dist/server.js . && dependency-check --extra --no-dev --entry dist/server.js ."
  },
  "repository": "https://github.com/stencila/cloud",
  "license": "Apache-2.0",
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jest": "^23.3.3",
    "dependency-check": "^3.2.1",
    "jest": "^23.6.0",
    "npm-check-updates": "^2.14.2",
    "standard": "^12.0.1",
    "ts-jest": "^23.10.4",
    "ts-node-dev": "^1.0.0-pre.30",
    "tslint": "^5.11.0",
    "tslint-config-standard": "^8.0.1",
    "typedoc": "^0.12.0",
    "typescript": "^3.1.1"
  },
  "dependencies": {
    "@sentry/node": "^4.6.3",
    "body-parser": "^1.18.3",
    "express": "^4.16.3",
    "kubernetes-client": "^6.1.0",
    "pino": "^5.7.0",
    "retry-request": "^4.0.0"
  }
}
