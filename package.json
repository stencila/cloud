{
  "name": "sibyl-open",
  "version": "1.0.0",
  "description": "Launch Sibyl containers",
  "main": "index.js",
  "scripts": {
    "test": "standard",
    "watch": "bankai build --watch client/index.js dist/",
    "build": "NODE_ENV=production bankai build --verbose client/index.js dist/",
    "start": "PORT=3000 TOKEN_SECRET='mysecret' nodemon --ignore dist/ --ignore client/ --ignore bundles/ server/server.js | merry",
    "serve": "node server/server.js"
  },
  "repository": "stencila/open",
  "license": "Apache-2.0",
  "devDependencies": {
    "bankai": "^8.1.1",
    "nodemon": "^1.11.0",
    "standard": "^10.0.2"
  },
  "dependencies": {
    "array-lru": "^1.1.1",
    "cache-component": "^5.2.0",
    "choo": "^5.6.2",
    "choo-expose": "^1.2.1",
    "choo-log": "^7.0.1",
    "cookie": "^0.3.1",
    "fast-json-parse": "^1.0.2",
    "jsonwebtoken": "^7.4.1",
    "merry": "^5.3.3",
    "parse-formdata": "^1.0.2",
    "pump": "^1.0.2",
    "send": "^0.15.3",
    "shallowequal": "^1.0.2",
    "sheetify": "^6.1.0",
    "tachyons": "^4.7.4",
    "uuid": "^3.1.0",
    "xhr": "^2.4.0"
  }
}
